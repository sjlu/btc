div.form-group
  label(for="rate")
    | Frame Rate
  select.form-control(id="rate" ng-model="ngModel" ng-change="ngChange()")
    option(value="60")
      | 1 minute
    option(value="300")
      | 5 minutes
    option(value="600")
      | 10 minutes
    option(value="900")
      | 15 minutes
    option(value="1800")
      | 30 minutes
    option(value="3600")
      | 1 hour